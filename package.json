{
	"name": "angular-seed",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build -c development",
		"build:qa": "ng build -c qa",
		"build:uat": "ng build -c uat",
		"build:prod": "ng build -c production",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"i18n:init": "ngx-translate-extract -i ./src -o ./src/assets/i18n/template.json -k -r -f namespaced-json",
		"i18n:extract": "ngx-translate-extract -i ./src -o ./src/assets/i18n/da.json -o ./src/assets/i18n/en.json -k -c --fi -f namespaced-json"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~10.2.0",
		"@angular/common": "~10.2.0",
		"@angular/compiler": "~10.2.0",
		"@angular/core": "~10.2.0",
		"@angular/forms": "~10.2.0",
		"@angular/platform-browser": "~10.2.0",
		"@angular/platform-browser-dynamic": "~10.2.0",
		"@angular/router": "~10.2.0",
		"@ngrx/effects": "^10.1.0",
		"@ngrx/entity": "^10.1.0",
		"@ngrx/store": "^10.1.0",
		"@ngrx/store-devtools": "^10.1.0",
		"@ngx-translate/core": "^13.0.0",
		"@ngx-translate/http-loader": "^6.0.0",
		"@types/pako": "^1.0.1",
		"ng-zorro-antd": "^10.1.2",
		"pako": "^2.0.2",
		"rxjs": "~6.6.0",
		"tslib": "^2.0.0",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1002.0",
		"@angular/cli": "~10.2.0",
		"@angular/compiler-cli": "~10.2.0",
		"@biesbjerg/ngx-translate-extract": "^7.0.3",
		"@types/jasmine": "~3.5.0",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^12.11.1",
		"codelyzer": "^6.0.0",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~5.0.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"ngrx-store-freeze": "^0.2.4",
		"protractor": "~7.0.0",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"typescript": "~4.0.2"
	}
}
